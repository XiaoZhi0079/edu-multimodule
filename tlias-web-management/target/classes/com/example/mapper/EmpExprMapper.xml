<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.EmpExprMapper">

<!--    批量插入-->
    <insert id="addEmpExprsBatch">
        insert into emp_expr(emp_id,begin,end,company,job)
        values
        <foreach collection="empExprsList" item="empExpr" separator=",">
            (#{empExpr.empId},#{empExpr.begin},#{empExpr.end},#{empExpr.company},#{empExpr.job})
        </foreach>
    </insert>
<!--    删除员工工作经历信息-->
    <delete id="deleteByempid">
        delete from emp_expr where emp_id in
        <foreach item="empid" collection="empids" separator="," open="(" close=")">
            #{empid}
        </foreach>
    </delete>

</mapper>